<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="sample.controller.AssignProjectController">
    <children>

        <Label text="Assign Project" underline="true" AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="8.0">
            <font>
                <Font name="System Bold" size="21.0"/>
            </font>
            <padding>
                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
            </padding>
        </Label>
        <ScrollPane nodeOrientation="LEFT_TO_RIGHT" prefHeight="300.0" AnchorPane.leftAnchor="16.0"
                    AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="60.0">
            <VBox fx:id="vbox">
                <children>
                </children>
            </VBox>
        </ScrollPane>
        <Button fx:id="submitButton" mnemonicParsing="false" onAction="#onAssignSubmitClicked" text="Continue"
                AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="380.0"/>
    </children>

</AnchorPane>
